<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kbo.board.repository.BoardMapper">
    <select id="selectBoardList" resultType="com.kbo.board.dto.BoardDTO">
        SELECT ID AS ID,
               TITLE AS TITLE,
               CONTENT AS CONTENT
        FROM  BOARD
    </select>
</mapper>